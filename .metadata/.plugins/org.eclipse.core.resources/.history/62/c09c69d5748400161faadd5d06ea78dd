//It is a program that is able to convert temperature in degree Fahrenheit into degree Celsius.
public class TempConvert {

	public static void main(String[] args) {
		double Fahrenheit = 32.0;
		double Celsius;
		Celsius = (5.0/9.0) * (Fahrenheit - 32.0);
		System.out.println ("The degree Celsius is" + Celsius);

	}

}
